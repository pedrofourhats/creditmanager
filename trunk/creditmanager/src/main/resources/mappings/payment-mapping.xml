<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
	<mapping wildcard="false">
		<class-a>com.creditmanager.model.Payment</class-a>
		<class-b>com.creditmanager.service.dto.PaymentDTO</class-b>

		<field>
			<a>id</a>
			<b>id</b>
		</field>
		<field>
			<a>feeNumber</a>
			<b>feeNumber</b>
		</field>
		<field>
			<a>feeAmountPaid</a>
			<b>feeAmountPaid</b>
		</field>
		<field>
			<a>paymentDate</a>
			<b>paymentDate</b>
		</field>
		<field>
			<a>rate</a>
			<b>rate</b>
		</field>
	</mapping>

</mappings>